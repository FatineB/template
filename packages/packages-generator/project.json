{
  "root": "packages/packages-generator",
  "sourceRoot": "packages/packages-generator/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/packages-generator",
        "main": "packages/packages-generator/src/index.ts",
        "tsConfig": "packages/packages-generator/tsconfig.lib.json",
        "assets": [
          "packages/packages-generator/*.md",
          {
            "input": "./packages/packages-generator/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/packages-generator/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/packages-generator",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/packages-generator",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/packages-generator/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/packages-generator"],
      "options": {
        "jestConfig": "packages/packages-generator/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
